<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="org.tensorflow.lite.examples.posenet"
    android:versionCode="1"
    android:versionName="1.0" >

    <uses-sdk
        android:minSdkVersion="23"
        android:targetSdkVersion="30" />

    <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.CAMERA" />

    <uses-feature android:name="android.hardware.camera" />
    <uses-feature android:name="android.hardware.camera.autofocus" />

    <application
        android:allowBackup="true"
        android:appComponentFactory="androidx.core.app.CoreComponentFactory"
        android:debuggable="true"
        android:extractNativeLibs="false"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/tfe_pn_app_name"
        android:requestLegacyExternalStorage="true"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:testOnly="true"
        android:theme="@style/AppTheme.Posenet" >
        <activity android:name="org.tensorflow.lite.examples.posenet.MusicActivity" />
        <activity android:name="org.tensorflow.lite.examples.posenet.suryanamaskar" />
        <activity android:name="org.tensorflow.lite.examples.posenet.MainActivity" />
        <activity android:name="org.tensorflow.lite.examples.posenet.CameraActivity" />
        <activity
            android:name="org.tensorflow.lite.examples.posenet.TestActivity"
            android:exported="true"
            android:label="@string/tfe_pn_app_name" />
        <activity
            android:name="org.tensorflow.lite.examples.posenet.introActivity"
            android:exported="true"
            android:theme="@style/Theme.AppCompat.Light.NoActionBar" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <meta-data
            android:name="preloaded_fonts"
            android:resource="@array/preloaded_fonts" />

        <provider
            android:name="com.squareup.picasso.PicassoProvider"
            android:authorities="org.tensorflow.lite.examples.posenet.com.squareup.picasso"
            android:exported="false" />
    </application>

</manifest>